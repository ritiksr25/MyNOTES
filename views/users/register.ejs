<% include ../partials/head %>

  <div class="container"> 
<center>
  <form class="form" action="/users/register" method="post">
  <h1 class="h3 mb-3 font-weight-normal">Register</h1><br>
  <input type="text" style="width:50%" name="name" class="form-control" placeholder="Name" required autofocus><br>    
  <input type="email" style="width:50%" name="username" class="form-control" placeholder="Email ID" required autofocus><br>
  <input type="password" style="width:50%" name="password" class="form-control" placeholder="Password" required><br>
  <input type="password" style="width:50%" name="password2" class="form-control" placeholder="Confirm Password" required><br>
  <button class="btn btn-lg btn-primary btn-block" style="width:50%"  type="submit">Register</button>
  </form>
</center>
  </div>
</div>

<% include ../partials/footer %>






